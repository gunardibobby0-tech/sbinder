modules = ["nodejs-20", "web", "postgresql-16"]
[nix]
channel = "stable-25_05"
packages = ["unzip"]

[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:2.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev-server"

[[workflows.workflow]]
name = "dev-server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ./Binder-Replicatezipzip-1zip/Binder-Replicatezipzip/Binder-Replicatezip/Binder-Replicate && npm run dev"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80
exposeLocalhost = true
